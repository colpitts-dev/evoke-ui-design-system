{
  "name": "@evoke-ui/docs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "evoke:styles": "npx tailwindcss -c ../../packages/evoke-ui-core/tailwind.config.js -i ../../packages/evoke-ui-core/src/styles/main.scss -o .storybook/assets/styles.css",
    "dev": "yarn evoke:styles && start-storybook -p 6006",
